[
  {
    "name": "strip should strip whitespace from strings",
    "mapping": "output = input.strip()",
    "input": {"input": "  hello world  \t\n"},
    "output": {"output": "hello world"}
  },
  {
    "name": "strip should strip weird whitespace",
